{
    "manifest_version":2,
    "version":"0.1",
    "name":"Egghead.io Downloader",
    "content_scripts":[
        {
            "matches":["<all_urls>"],
            "js":[
                "jquery.min.js",
                "content.js"
            ]
        }
    ],

    "browser_action":{
        "default_icon": "icon.png",
        "default_popup": "popup.html",
        "default_title": "Get Link Current Course"
    },

    "background":{
        "scripts":[
            "bg.js"
        ]
    },

    "permissions":["tabs"]

}